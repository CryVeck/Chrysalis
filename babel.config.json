{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, last 2 years, not dead"
      }
    ],
    [
      "@babel/preset-react",
      {
        "debug": false,
        "targets": {
          "web": "current"
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-modules-commonjs"]
}
